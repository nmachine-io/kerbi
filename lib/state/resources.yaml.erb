<% consts = Kerbi::State::Consts %>

<% entries_json = JSON.dump(values[consts::ENTRIES_ATTR]) %>

apiVersion: v1
kind: ConfigMap
metadata:
  name: <%= values[:cm_name] %>
  namespace: <%= values[:namespace] %>
data: <%= embed({consts::ENTRIES_ATTR => entries_json}) %>

---

apiVersion: v1
kind: Namespace
metadata:
  name: <%= values[:namespace] %>
